{
	"compilerOptions": {
		"allowJs": true,
		"baseUrl": ".",
		"esModuleInterop": true,
		"lib": ["esnext"],
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"noEmit": true,
		"paths": {
			"@server/*": ["./server/*"],
			"http://*": ["../../Library/Caches/deno/deps/http/*"],
			"https://*": ["../../Library/Caches/deno/deps/https/*"]
		},
		"pretty": true,
		"resolveJsonModule": true
	},
	"include": ["**/*.ts", "**/*.tsx", "**/*.d.ts"]
}
